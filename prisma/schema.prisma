generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model products {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  description String
  image       String
  name        String
  price       Int
  createdAt DateTime @default(now())
  updateAt DateTime @updatedAt

  @@map("products")
}
